<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.pingdum.models">

        <class name="com.pingdum.models.Status" table="statuses">
            <id name="id" type="int" column="id">
                <generator class="native" />
            </id>
            <property name="siteId" column="siteId" type="int">
            </property>
            <property name="status" column="httpCode" type="int">
            </property>
            <!--<property name="timeStamp" type="timestamp">-->
                <!--<column name="timeStamp"/>-->
            <!--</property>-->
        </class>

</hibernate-mapping>